      <RulesElementTally>
         <RulesElement name="1" type="Level" internal-id="ID_INTERNAL_LEVEL_1" charelem="7" legality="rules-legal" >
            <specific name="XP Needed" > 1000 </specific>
         </RulesElement>
         <RulesElement name="Level1Rules" type="Level1Rules" internal-id="ID_INTERNAL_LEVEL1RULES_LEVEL1RULES" charelem="37" legality="rules-legal" >
